{
  "name": "wedding-website-client",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "dev:https": "vite --host --https",
    "build": "tsc --project tsconfig.build.json && vite build",
    "build:strict": "tsc --noEmit && vite build",
    "build:analyze": "ANALYZE=true npm run build",
    "analyze": "npm run build:analyze && echo 'Bundle analysis complete! Check dist/bundle-analysis.html'",
    "typecheck": "tsc --noEmit",
    "preview": "vite preview",
    "lint": "eslint src/**/*.{ts,tsx}",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "performance:build": "npm run build && npm run performance:check",
    "performance:check": "node scripts/performance-budget.js"
  },
  "dependencies": {
    "@apollo/client": "^3.13.8",
    "graphql": "^16.8.1",
    "html5-qrcode": "^2.3.8",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-icons": "^5.5.0",
    "react-router-dom": "^6.0.0",
    "web-vitals": "^5.1.0"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.2.74",
    "@types/react-dom": "^18.2.24",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/coverage-c8": "^0.33.0",
    "eslint": "^8.55.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jsdom": "^26.1.0",
    "prettier": "^3.1.0",
    "rollup-plugin-visualizer": "^6.0.3",
    "sharp": "^0.34.3",
    "typescript": "^5.0.0",
    "vite": "^5.4.20",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-imagemin": "^0.6.1",
    "vite-plugin-pwa": "^0.20.5",
    "vitest": "^0.30.1"
  }
}
