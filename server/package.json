{
  "name": "wedding-website-server",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "ts-node-dev --esm src/server.ts",
    "start": "node dist/server.js",
    "build": "tsc --project tsconfig.build.json",
    "build:strict": "tsc --noEmit && tsc --project tsconfig.build.json",
    "typecheck": "tsc --noEmit",
    "watch": "nodemon --watch src --ext ts,json --exec \"npm run build && npm run start\"",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "seed": "node dist/seeds/index.js",
    "seed-test": "MONGODB_DB_NAME=djforever2_test node dist/seeds/index.js",
    "seed-prod": "MONGODB_DB_NAME=djforever2 node dist/seeds/index.js",
    "clean:db": "node dist/seeds/cleanDB.js",
    "convert:guests": "node src/seeds/convertGuestList.js",
    "generate:qrcodes": "node dist/seeds/generateQRCodes.js",
    "generate:qrcodes:dev": "MONGODB_DB_NAME=djforever2_dev FRONTEND_URL=http://192.168.1.64:3002 node dist/seeds/generateQRCodes.js",
    "generate:qrcodes:prod": "MONGODB_DB_NAME=djforever2 FRONTEND_URL=https://dj-forever2.onrender.com NODE_ENV=production node dist/seeds/generateQRCodes.js",
    "migrate:hasrsvped": "npx ts-node --esm src/scripts/updateHasRSVPed.ts",
    "migrate:legacy-rsvps": "npm run build && MONGODB_DB_NAME=djforever2_dev node dist/scripts/migrateLegacyRSVPs.js",
    "migrate:user-personalization": "npm run build && node dist/scripts/migrateUserPersonalizationFields.js",
    "migrate:user-personalization:dev": "npm run build && MONGODB_DB_NAME=djforever2_dev node dist/scripts/migrateUserPersonalizationFields.js",
    "migrate:user-personalization:prod": "npm run build && MONGODB_DB_NAME=djforever2 node dist/scripts/migrateUserPersonalizationFields.js"
  },
  "dependencies": {
    "@apollo/server": "^4.12.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^20.11.5",
    "@types/nodemailer": "^7.0.2",
    "@types/qrcode": "^1.5.5",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.18.0",
    "express-rate-limit": "^8.2.1",
    "graphql": "^16.8.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.16.0",
    "nodemailer": "^7.0.9",
    "qrcode": "^1.5.4"
  },
  "devDependencies": {
    "@types/supertest": "^6.0.3",
    "nodemon": "^3.0.3",
    "supertest": "^7.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3",
    "vitest": "^0.30.1"
  }
}
