{
  "name": "wedding-website-server",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "ts-node-dev src/server.ts",
    "start": "node dist/server.js",
    "build": "tsc",
    "watch": "nodemon --watch src --ext ts,json --exec \"npm run build && npm run start\"",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "seed": "node dist/seeds/index.js",
    "seed-test": "MONGODB_DB_NAME=test_db node dist/seeds/index.js",
    "seed-prod": "MONGODB_DB_NAME=djforever2 node dist/seeds/index.js",
    "clean:db": "node dist/seeds/cleanDB.js",
    "generate:qrcodes": "node dist/seeds/generateQRCodes.js"
  },
  "dependencies": {
    "@apollo/server": "^4.12.2",
    "@types/jsonwebtoken": "^9.0.9",
    "apollo-server-express": "^3.13.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "express": "^4.18.0",
    "graphql": "^16.8.1",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.5",
    "@types/qrcode": "^1.5.5",
    "@types/supertest": "^6.0.3",
    "dotenv": "^16.5.0",
    "mongoose": "^8.16.0",
    "nodemon": "^3.0.3",
    "supertest": "^7.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.0",
    "vitest": "^0.30.1"
  }
}
